												<div class="form-group is-empty">
													<input type="text"
														placeholder="Enter name, email, or phone"
														class="form-control" autocomplete="off">
												</div>
											</div>
										</li>

										<c:set var="count" value="0" scope="page" />
										<c:forEach items="${users}" var="user" varStatus="loop">
											<c:set var="count" value="${count + 1}" scope="page" />
											<li class="list-group-item"
												onclick="getMessageWindow('${user.fullName}','${user.pid}','${user.ukMessageKey}','${user.id}',${count + 1})">
												<div>
													<span class="pull-left"><img
														src="<spring:url value="/resources/assets/img/profiles/0${count + 1}.jpg"/>"
														alt="" class="img-circle img-sm m-r-10"></span> <i
														class="fa fa-commenting-o" id='${user.ukMessageKey}'
														style="float: right; display: none;color: blue;"></i>
												</div>
												<div class="list-group-item-body">
													<div class="list-group-item-heading">${user.fullName}</div>
													<div class="list-group-item-text">${user.userName}</div>

												</div>
											</li>
										</c:forEach>

										<c:forEach items="${group}" var="group">

